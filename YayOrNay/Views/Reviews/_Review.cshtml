@model YayOrNay.Models.MovieReview

<div class="review">
    <h4>@Model.Title</h4>
    <p>@Model.Rating / 10</p>
    <p>@Model.Genre</p>
    <p>@Model.Certificate</p>
    <p>@Model.ReleaseDate</p>


    <span class="right">
        @Html.ActionLink("Edit", "Edit", new { id = Model.Id })
    </span>
</div>
